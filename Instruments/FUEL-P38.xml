<PropertyList>

    <path>Models/fuel-p38.ac</path>
    <texture-path>Models/Textures</texture-path>

        <animation>
                <type>noshadow</type>
                <object-name>FUEL</object-name>
        </animation>





        <animation include="Instrument-Light4.xml">
                <object-name>Aiguille</object-name>
        </animation>

        <animation include="Instrument-Light3.xml">
                <object-name>Cadran</object-name>
        </animation>

        <animation>
                <type>rotate</type>
                <object-name>Aiguille</object-name>
                <property>/consumables/fuel/total-fuel-gals</property>
                <factor>0.51800052</factor>
                <!--80 degres=1000lbs=154.44gal_us-->
                <!--<interpolation>
                        <entry>
                                <ind>0</ind>
                                <dep>0</dep>
                        </entry>
                        <entry>
                                <ind>4000</ind>
                                <dep>320</dep>
                        </entry>
                </interpolation>-->
                <axis>
                        <x>-1</x>
                        <y>0</y>
                        <z>0</z>
                </axis>
        </animation>

</PropertyList>